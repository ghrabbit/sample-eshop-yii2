	<div class="col-xs-12 col-sm-6 col-md-4">
		<div class="thumbnail">
			
			<div class="clicimg">
				<img tabindex="1" src="/images/product/small/{{model.img_file}}"></img>
				<dl>
					<dt></dt>
					<dd><img src="/images/product/norm/{{model.img_file}}"></img><p><h4>{{model.title}}</h4></p></dd>
				</dl>
			</div>
			<div class="caption">
				<table><td height="60">
				<h4><a href="/shopping/product_details/{{model.id}}">{{^model.title}}noname{{/model.title}}{{model.title}}</a></h4>
				</td></table>
				<p>
				{{#button}}
				<button class="btn btn-primary add-to-cart" data-toggle="tooltip" data-placement="top" title="{{labels.addToCart}}">
								<i class="glyphicon glyphicon-shopping-cart"></i> <span class="badge"> {{{model.fprice}}}</span></button> 
				<span class="pull-right">
							<button	class="btn btn-default product-details" data-toggle="tooltip" data-placement="top"  title="{{labels.productDetails}}"><i class="glyphicon glyphicon-paperclip"></i></button>
				</span>
				{{/button}}
				{{^button}}
				<a href="{{baseUrl}}/shopping/cartAdd/{{model.id}}" class="btn btn-primary" data-toggle="tooltip" data-placement="top" title="{{labels.addToCart}}">
								<i class="glyphicon glyphicon-shopping-cart"></i> <span class="badge"> {{{model.fprice}}}</span></a> 
					
				<span class="pull-right">
							<a href="{{baseUrl}}/catalog/productDetails/{{model.id}}" 
							class="btn btn-default" data-toggle="tooltip" data-placement="top"  title="{{labels.productDetails}}"><i class="glyphicon glyphicon-paperclip"></i></a>
				</span>
				{{/button}}
				</p>
			</div>
			
		</div>	
	</div>

			
		
